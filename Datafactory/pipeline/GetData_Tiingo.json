{
	"name": "GetData_Tiingo",
	"properties": {
		"activities": [
			{
				"name": "Copy Tiingo API Data",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "RestSource",
						"httpRequestTimeout": "00:01:40",
						"requestInterval": "00.00:00:00.010",
						"requestMethod": "GET"
					},
					"enableStaging": false
				},
				"inputs": [
					{
						"referenceName": "TiingoAPI",
						"type": "DatasetReference",
						"parameters": {
							"TiingoTargetDataURL": {
								"value": "@concat(\n'daily/',\npipeline().parameters.TiingoTicker,\n'/prices?startDate=',\npipeline().parameters.TiingoStartDate,\n'&endDate=',\nutcnow('yyy-MM-dd')\n)",
								"type": "Expression"
							}
						}
					}
				]
			}
		],
		"parameters": {
			"TiingoTicker": {
				"type": "string"
			},
			"TiingoStartDate": {
				"type": "string"
			}
		},
		"annotations": []
	}
}